package com.aksh.multi.threading;

class Medical extends Thread {

	public void run() {
		try {
			System.out.println("Medical Start.");
			Thread.sleep(2000);
			System.out.println("Medical Completed.");
		} catch (Exception e) {
			// TODO: handle exception
		}
	}
}

