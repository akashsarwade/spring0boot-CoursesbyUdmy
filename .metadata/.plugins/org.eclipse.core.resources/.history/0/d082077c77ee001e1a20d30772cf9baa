package com.java.basic.dup;

import java.util.HashMap;
import java.util.Map;import javax.crypto.spec.ChaCha20ParameterSpec;

public class DuplicateChar {
	
	
	public static void main(String[] args) {
		
	String str="aabc";
	public static  void findDuplicate(str);

	}
	
	
	public static findDuplicate(String str) {
		Map<Character, Integer>charCountMap=new  HashMap<>();
	
		for(char ch : toCharArray()) {
			if(charCountMap.containsKey(ch)) {
				charCountMap.put(ch, charCountMap.get(ch)+1);
				
			}else {
				charCountMap.put(ch, 1);
			}
		
		}
		System.out.println("Duplicate:");
		for(Map.Entry<Character, Integer>enty:charCountMap.entrySet())
		{
		
			if(enty.getValue()>1) {
				System.out.println(enty.getKey());
			}
		}
		}
	
	
	
	
	
	
}
